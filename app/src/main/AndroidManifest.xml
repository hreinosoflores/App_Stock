<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app_stock">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".controllers.Login"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".controllers.DetalleProducto"
            android:configChanges="orientation" />
        <activity
            android:name=".controllers.EditarProducto"
            android:configChanges="orientation" />
        <activity
            android:name=".controllers.RegistrarProducto"
            android:configChanges="orientation" />
        <activity
            android:name=".controllers.ListaProducto"
            android:configChanges="orientation" />

    </application>

</manifest>